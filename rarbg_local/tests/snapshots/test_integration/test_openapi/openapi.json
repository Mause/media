  "components": {
    "schemas": {
      "DownloadAllResponse": {
        "title": "DownloadAllResponse",
        "required": [
          "packs",
          "complete",
          "incomplete"
        ],
        "type": "object",
        "properties": {
          "packs": {
            "title": "Packs",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ITorrent"
            }
          },
          "complete": {
            "title": "Complete",
            "type": "array",
            "items": {
              "type": "array",
              "minItems": 2,
              "maxItems": 2,
              "items": [
                {
                  "type": "string"
                },
                {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ITorrent"
                  }
                }
              ]
            }
          },
          "incomplete": {
            "title": "Incomplete",
            "type": "array",
            "items": {
              "type": "array",
              "minItems": 2,
              "maxItems": 2,
              "items": [
                {
                  "type": "string"
                },
                {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ITorrent"
                  }
                }
              ]
            }
          }
        }
      },
      "DownloadPost": {
        "title": "DownloadPost",
        "required": [
          "tmdb_id",
          "magnet"
        ],
        "type": "object",
        "properties": {
          "tmdb_id": {
            "title": "Tmdb Id",
            "type": "integer"
          },
          "magnet": {
            "title": "Magnet",
            "pattern": "^magnet:",
            "type": "string"
          },
          "season": {
            "title": "Season",
            "type": "string"
          },
          "episode": {
            "title": "Episode",
            "type": "string"
          }
        }
      },
      "DownloadSchema": {
        "title": "DownloadSchema",
        "required": [
          "id",
          "tmdb_id",
          "transmission_id",
          "imdb_id",
          "type",
          "title",
          "timestamp",
          "added_by"
        ],
        "type": "object",
        "properties": {
          "id": {
            "title": "Id",
            "type": "integer"
          },
          "tmdb_id": {
            "title": "Tmdb Id",
            "type": "integer"
          },
          "transmission_id": {
            "title": "Transmission Id",
            "type": "string"
          },
          "imdb_id": {
            "title": "Imdb Id",
            "type": "string"
          },
          "type": {
            "title": "Type",
            "type": "string"
          },
          "title": {
            "title": "Title",
            "type": "string"
          },
          "timestamp": {
            "title": "Timestamp",
            "type": "string",
            "format": "date-time"
          },
          "added_by": {
            "$ref": "#/components/schemas/UserSchema"
          }
        }
      },
      "Episode": {
        "title": "Episode",
        "required": [
          "name",
          "id",
          "episode_number"
        ],
        "type": "object",
        "properties": {
          "name": {
            "title": "Name",
            "type": "string"
          },
          "id": {
            "title": "Id",
            "type": "integer"
          },
          "episode_number": {
            "title": "Episode Number",
            "type": "integer"
          },
          "air_date": {
            "title": "Air Date",
            "type": "string",
            "format": "date"
          }
        }
      },
      "EpisodeDetailsSchema": {
        "title": "EpisodeDetailsSchema",
        "required": [
          "id",
          "download",
          "show_title",
          "season"
        ],
        "type": "object",
        "properties": {
          "id": {
            "title": "Id",
            "type": "integer"
          },
          "download": {
            "$ref": "#/components/schemas/DownloadSchema"
          },
          "show_title": {
            "title": "Show Title",
            "type": "string"
          },
          "season": {
            "title": "Season",
            "type": "integer"
          },
          "episode": {
            "title": "Episode",
            "type": "integer"
          }
        }
      },
      "EpisodeInfo": {
        "title": "EpisodeInfo",
        "type": "object",
        "properties": {
          "seasonnum": {
            "title": "Seasonnum",
            "type": "string"
          },
          "epnum": {
            "title": "Epnum",
            "type": "string"
          }
        }
      },
      "HTTPValidationError": {
        "title": "HTTPValidationError",
        "type": "object",
        "properties": {
          "detail": {
